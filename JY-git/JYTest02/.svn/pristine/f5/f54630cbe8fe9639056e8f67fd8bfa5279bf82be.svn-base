}
#form {
	height: 300px;
	padding-top: 60px;
	padding-right: 20px;
	padding-bottom: 20px;
	padding-left: 10px;
	background-color:none;
}
#form img{ 
	float:left;
	margin:0 0 20 40px; /*上 右  下  左*/
	width:300px; 
	height:100px; 
 	
}
#form table {
	border-top-style: none;
	border-right-style: none;
	border-bottom-style: none;
	border-left-style: none;
 	background-color:none;
}
#form table tr {
	font-family: Arial;
	font-size: 20px;
	line-height: 25px;
	font-weight: bolder;
	border-top-style: none;
	border-right-style: none;
	border-bottom-style: none;
	border-left-style: none;
}
#form table td {
	border-top-style: none;
	border-right-style: none;
	border-bottom-style: none;
	border-left-style: none;
}
#form input {
	height: 40px;
	width: 300px;
//	margin: 0px;
	border: 0px double #ccc;
	padding-left:10px;
	margin-bottom:1px;
	font-family: 幼圆;
	font-size: 16px;
	font-style: normal;
	font-weight: bold;
	line-height: 20px;
	color: #666666;
	border-radius:3%; 
	background-color:rgba(255, 255, 255, 0.7);
}
.error{
	line-height: 16px;
	font-size: 14px; 
	color: #CD0000; 
	font-family: 幼圆;
	padding-left:10px;
	height:16px;
 	margin:0 0 0 80px;
}
.STYLE4 {
	font-size: 22px; 
	color: #000; 
	font-weight: bold; 
	letting-space:2px;
	font-family: 黑体;
}
.STYLE5 {
	font-size: 18px;
	color: black; 
	font-family: 幼圆;
}